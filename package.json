    "clean": "rimraf dist",
    "serve": "vite preview"
  },
  "dependencies": {
    "@types/nprogress": "^0.2.0",
    "@vueuse/core": "^7.7.0",
    "@vueuse/head": "^0.7.5",
    "ant-design-vue": "^3.0.0-alpha.16",
    "axios": "^0.21.4",
    "lodash": "^4.17.21",
    "pinia": "^2.0.11",
    "vue": "^3.2.21",
    "vue-i18n": "^9.1.9",
    "vue-request": "^1.2.4",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@iconify/vue": "^3.1.3",
    "@intlify/vite-plugin-vue-i18n": "^3.3.0",
    "@types/good-storage": "^1.1.0",
    "@types/node": "^17.0.19",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/compiler-sfc": "^3.2.11",
    "cos-js-sdk-v5": "^1.3.4",
    "dayjs": "^1.10.7",
    "echarts": "^5.2.2",
    "good-storage": "^1.1.1",
    "less": "^4.1.1",
    "markdown-it-prism": "^2.2.3",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "sass": "^1.39.0",
    "typescript": "^4.5.5",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-vue-components": "^0.17.19",
    "uuid": "^8.3.2",
    "vite": "^2.8.4",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-html": "^3.1.0",
    "vite-plugin-md": "^0.11.8",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.20.2",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vue-tsc": "^0.31.4",
    "wangeditor": "^4.7.7"
  },
  "engines": {
    "node": ">=13.14.0",
    "yarn": ">=1.22.10"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@iconify/vue",
        "@vueuse/core",
        "@vueuse/head",
        "clipboard",
        "lodash",
        "pinia",
        "vue",
        "vue-i18n",
        "vue-request",
        "vue-router",
        "windicss"
      ]
    }
  }
}
